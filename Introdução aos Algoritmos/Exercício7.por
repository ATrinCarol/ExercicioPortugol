programa
{
	
	funcao inicio()
	{
		inteiro a, b, c, d, E, f, x, y

	a = 1
	b = 2
	c = 3
	d = 4
	E = 5
	f = 6

	 
		x = ((c+E - b+f) / (a+E - b+d))
		y = ((a+f - c+d) / (a+E - b+d))
		escreva ("O valor de X é: ", x, ", e o valor de Y é: ", y, ".")
	}
}
